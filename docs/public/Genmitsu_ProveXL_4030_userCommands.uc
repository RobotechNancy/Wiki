[{"hint":"Zero XY","imageName":"axis_zero.png","imageBase64":"","type":"0","code":"G92 X0 Y0"},{"hint":"Zero Z","imageName":"zero_z.png","imageBase64":"","type":"0","code":"G92 Z0"},{"hint":"Coordinates","imageName":"run.png","imageBase64":"","type":"1","code":"script.importExtension(\"qt.widgets\");\n\nconst dialog = new QDialog(app.window);\nconst layoutMain = new QVBoxLayout(dialog);\nconst layoutCoords = new QGridLayout();\nconst layoutButtons = new QHBoxLayout();\nconst lblX = new QLabel(\"X:\",dialog);\nconst lblY = new QLabel(\"Y:\",dialog);\nconst sbpX = new QSpinBox(dialog);\nconst sbpY = new QSpinBox(dialog);\nconst radAbsolute = new QRadioButton(\"Absolute (G90)\",dialog);\nconst radRelative = new QRadioButton(\"Relative (G91)\",dialog);\nconst radMachine = new QRadioButton(\"Machine (G53)\",dialog);\nconst cmdMove = new QPushButton(\"Move\",dialog);\nconst cmdClose = new QPushButton(\"Close\",dialog);\n\nlayoutCoords.addWidget(lblX, 0, 0);\nlayoutCoords.addWidget(lblY, 1, 0);\nlayoutCoords.addWidget(sbpX, 0, 1);\nlayoutCoords.addWidget(sbpY, 1, 1);\n\nlayoutButtons.addWidget(cmdMove, 0, 0);\nlayoutButtons.addWidget(cmdClose, 0, 0);\n\nlayoutMain.addLayout(layoutCoords);\nlayoutMain.addWidget(radAbsolute, 0, 0);\nlayoutMain.addWidget(radRelative, 0, 0);\nlayoutMain.addWidget(radMachine, 0, 0);\nlayoutMain.addLayout(layoutButtons);\n\nsbpX.minimum= -10000;\nsbpX.maximum = 10000;\nsbpY.minimum= -10000;\nsbpY.maximum = 10000;\n\nradAbsolute.checked = true;\n\ncmdMove.autoDefault = true;\ncmdMove.clicked.connect(function(){\napp.device.sendCommand(\n(radAbsolute.checked\n? \"G90\"\n:radRelative.checked\n? \"G91\"\n:\"G53\")\n+ \"G0\"\n+ \"X\"+ sbpX.value\n+ \"Y\"+ sbpY.value\n);\n});\n\ncmdClose.clicked.connect(function(){\ndialog.reject();\n});\n\ndialog.setWindowTitle(\"Move to coordinates\");\ndialog.setFixedSize(dialog.sizeHint);\ndialog.exec();"},{"hint":"Work origin","imageName":"origin.png","imageBase64":"","type":"0","code":"G90 G0 X0 Y0"},{"hint":"Probe X","imageName":"*search_for_x.png","imageBase64":"","type":"1","code":"app.device.storeParserState();\napp.device.sendCommands([\n\"G21 G91\",\n\"G38.2 X30 F100\",\n\"G0 X-1\",\n\"G38.2 X2 F10\",\n\"G92 X0\",\n\"G91 G0 X-5\",\n]);\napp.device.waitResponses();\napp.device.restoreParserState();"},{"hint":"Probe Y","imageName":"*search_for_y.png","imageBase64":"","type":"1","code":"app.device.storeParserState();\napp.device.sendCommands([\n\"G21 G91\",\n\"G38.2 Y-30 F100\",\n\"G0 Y1\",\n\"G38.2 Y-2 F10\",\n\"G92 Y0\",\n\"G91 G0 Y5\",\n]);\napp.device.waitResponses();\napp.device.restoreParserState();"},{"hint":"Probe Z","imageName":"*search_for_z.png","imageBase64":"","type":"1","code":"app.device.storeParserState();\napp.device.sendCommands([\n\"G21 G91\",\n\"G38.2 Z-30 F100\",\n\"G0 Z1\",\n\"G38.2 Z-2 F10\",\n\"G92 Z20\",\n\"G91 G0 Z5\"\n]);\napp.device.waitResponses();\napp.device.restoreParserState();"},{"hint":"Set Home","imageName":"*set_home.png","imageBase64":"","type":"0","code":"G28.1"}]